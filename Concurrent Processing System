class Task extends Thread {
    public void run() {
        for(int i=1;i<=3;i++)
            System.out.println(Thread.currentThread().getName()+" Running");
    }
}

public class Main {
    public static void main(String[] args) {
        Task t1 = new Task();
        Task t2 = new Task();
        t1.start();
        t2.start();
    }
}
